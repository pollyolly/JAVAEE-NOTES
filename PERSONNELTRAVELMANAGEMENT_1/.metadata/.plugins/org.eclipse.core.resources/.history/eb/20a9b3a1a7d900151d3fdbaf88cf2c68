$(document).ready(function() {
	$('.service').select2({
		
		  ajax: {
		    data: function (params) {
		      var query = {
		        search: params.term,
		        page: params.page
		      }
		      // Query paramters will be ?search=[term]&page=[page]
		      return query;
		    }
		  }
		});
});
