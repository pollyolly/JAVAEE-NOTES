$(document).ready(function() {
	$('.serviceprovider').change(function (){
		var spid = $(".serviceprovider option:selected").val();
		//var datas = [];
	    if (spid != null && spid != '')
	    {
	    		$.ajax({
	    			url : "TravelPlanList",
	    			type : "POST",
	    			data : {
	    				forms : "popuServices",
	    				ids : spid
	    			},
	    			dataType: "JSON",
	    			cache : "false",
	    			success : function(data) {
	    				$(".service").empty();
	    				$.each(data, function(i, item){
	    					$(".service").prepend('<option value="'+data[i].id+'">'+data[i].service+'</option>');
	    				});
	    			}
	    		});
	    }      
	});
});