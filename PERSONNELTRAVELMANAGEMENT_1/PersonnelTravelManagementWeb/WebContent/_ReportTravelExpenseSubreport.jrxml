	<parameter name="odid" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
      xplst.pcn,
	  typ.`type`,
	  xplst.`date`,
	  xplst.`reimburse`,
	  xplst.`billable`,
	  IFNULL(xplst.`reason`, "") as reason,
	  IFNULL(xplst.`amount`,0.00) as amount,
	  IFNULL(xplst.`vendor`,"") as vendor,
	/*MEAL*/
	IFNULL(ml.`vendor`,"") as mealvendor,
	IFNULL(ml.`description`,"") as mealdesc,
	/*MISCELLANEOUS*/
	IFNULL(misc.`vendor`,"") as miscvendor,
	IFNULL(misc.`description`,"") as miscdesc,
	/*FLIGHT*/
	IFNULL(fl.`from`,"") as ffrom, 
	IFNULL(fl.`to`,"") as fto, 
	IFNULL(fl.`departure`,"") as fdept, 
	IFNULL(fl.`return`,"") as fret, 
	fl.`stopover` as fstop,
	/*HOTEL*/  
	IFNULL(hl.`checkin`,"") as checkin, 
	IFNULL(hl.`checkout`,"") as checkout,
	/*MILEAGE*/
	IFNULL(mlge.`from`,"") as milefrom, 
	IFNULL(mlge.`to`,"") as mileto, 
	IFNULL(mlge.`distance`,"") as distance,
	/*CAR RENTAL*/
	IFNULL(cr.`from`,"") as carfrom, 
	IFNULL(cr.`to`,"") as carto, 
	IFNULL(cr.`pickup`,"") AS pickup, 
	IFNULL(cr.`dropoff`,"") as dropoff
	FROM `bps_ptms`.`ptms_t_travelexpenselist` AS xplst
	LEFT JOIN ptms_m_miscellaneous AS misc
	ON xplst.`id` = misc.`expenselistid`
	LEFT JOIN ptms_m_meal AS ml
	ON xplst.`id` = ml.`expenselistid`
	LEFT JOIN ptms_m_flight AS fl
	ON xplst.`id` = fl.`expenselistid`
	LEFT JOIN ptms_m_hotel	AS hl
	ON xplst.`id` = hl.`expenselistid`
	LEFT JOIN ptms_m_car AS cr
	ON xplst.`id` = cr.`expenselistid`
	LEFT JOIN ptms_m_mileage AS mlge
	ON xplst.`id` = mlge.`expenselistid`
	LEFT JOIN ptms_r_servicetype AS typ
	ON xplst.`typeid` = typ.`id`
	WHERE  xplst.`orderid` =  $P{odid}]]>
	</queryString>
