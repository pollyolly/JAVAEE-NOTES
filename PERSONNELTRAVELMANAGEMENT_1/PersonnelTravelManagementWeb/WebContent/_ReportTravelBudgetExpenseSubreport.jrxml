	<parameter name="tono" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	tos.tono,
	  typ.`type`,
	  SUM(xplst.`amount`) AS amnt
	FROM `bps_ptms`.`ptms_t_travelexpenselist` AS xplst
	LEFT JOIN ptms_r_servicetype AS typ
	ON xplst.`typeid` = typ.`id`
	LEFT JOIN (
		SELECT
		tos.`id`
		, CONCAT(REPLACE(REPLACE(REPLACE(tos.todate,'-',''),':',''),' ','')) AS tono
		FROM ptms_t_travelorder AS tos
	) AS tos
	ON xplst.`orderid` = tos.id
	WHERE tos.tono =   $P{tono} 
	GROUP BY typ.`type`]]>
	</queryString>
